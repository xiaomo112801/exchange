{
  "globalStyle": {
    "navigationBarBackgroundColor": "@navBgColor",
    "navigationBarTextStyle": "@navTxtStyle",
    "navigationBarTitleText": "Wot Starter",
    "backgroundColor": "@bgColor",
    "backgroundTextStyle": "@bgTxtStyle",
    "backgroundColorTop": "@bgColorTop",
    "backgroundColorBottom": "@bgColorBottom",
    "enablePullDownRefresh": false,
    "onReachBottomDistance": 50,
    "animationType": "pop-in",
    "animationDuration": 300
  },
  "pages": [
    // GENERATED BY UNI-PAGES, PLATFORM: APP || H5 || MP-WEIXIN
    {
      "path": "pages/index/index",
      "type": "home",
      "name": "index",
      "style": {
        "navigationBarTitleText": "首页",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/assets/index",
      "type": "page",
      "name": "assets",
      "style": {
        "navigationBarTitleText": "资产"
      },
      "middlewares": [
        "auth"
      ]
    },
    {
      "path": "pages/contract/index",
      "type": "page",
      "name": "contract",
      "style": {
        "navigationBarTitleText": "合约"
      }
    },
    {
      "path": "pages/index/search",
      "type": "page",
      "name": "search",
      "style": {
        "navigationBarTitleText": "搜索",
        "navigationStyle": "custom",
        "globalStyle": {
          "app-plus": {
            "animationType": "slide-in-bottom",
            "animationDuration": 300
          }
        }
      }
    },
    {
      "path": "pages/login/index",
      "type": "page",
      "name": "login",
      "style": {
        "navigationBarTitleText": "登录",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/market/index",
      "type": "page",
      "name": "market",
      "style": {
        "navigationBarTitleText": "市场"
      }
    },
    {
      "path": "pages/trade/index",
      "type": "page",
      "name": "trade",
      "style": {
        "navigationBarTitleText": "交易"
      }
    }
  ],
  "subPackages": [
    {
      "root": "subPages",
      "pages": [
        // GENERATED BY UNI-PAGES, PLATFORM: APP || H5 || MP-WEIXIN
        {
          "path": "feedback/index",
          "type": "page",
          "name": "feedback",
          "style": {
            "navigationBarTitleText": "反馈"
          }
        },
        {
          "path": "styles/index",
          "type": "page",
          "name": "styles",
          "style": {
            "navigationBarTitleText": "UnoCSS 演示"
          }
        }
      ]
    },
    {
      "root": "subEcharts",
      "pages": [
        // GENERATED BY UNI-PAGES, PLATFORM: APP || H5 || MP-WEIXIN
        {
          "path": "echarts/index",
          "type": "page",
          "name": "echarts",
          "style": {
            "navigationBarTitleText": "Echarts 演示"
          }
        }
      ]
    },
    {
      "root": "subAsyncEcharts",
      "pages": [
        // GENERATED BY UNI-PAGES, PLATFORM: APP || H5 || MP-WEIXIN
        {
          "path": "asyncEcharts/index",
          "type": "page",
          "name": "echartsAsync",
          "style": {
            "navigationBarTitleText": "echarts 异步跨包示例"
          }
        }
      ]
    },
    {
      "root": "pagesIndex",
      "pages": [
        // #ifdef APP || H5 || MP-WEIXIN
        {
          "path": "coin/detail",
          "type": "page",
          "name": "coinDetail",
          "style": {
            "navigationBarTitleText": "币种详情",
            "navigationStyle": "custom"
          }
        },
        // #endif
        {
          "path": "HotContractsList",
          "type": "page"
        },
        {
          "path": "notice/index",
          "type": "page",
          "name": "notice",
          "style": {
            "navigationBarTitleText": "公告",
            "navigationStyle": "custom"
          }
        },
        {
          "path": "recharge/index",
          "type": "page",
          "name": "recharge",
          "style": {
            "navigationBarTitleText": "充值"
          }
        },
        {
          "path": "slider/index",
          "type": "page",
          "name": "slider",
          "style": {
            "navigationBarTitleText": "Slider"
          },
          "subPackages": [
            {
              "root": "pagesIndex",
              "pages": [
                {
                  "path": "slider/index",
                  "name": "slider"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "tabBar": {
    "custom": true,
    "customize": true,
    "overlay": true,
    "height": "0",
    "color": "@tabColor",
    "selectedColor": "@tabSelectedColor",
    "backgroundColor": "@tabBgColor",
    "borderStyle": "@tabBorderStyle",
    "list": [
      // #ifdef APP || H5 || MP-WEIXIN
      {
        "pagePath": "pages/index/index"
      },
      // #endif
      {
        "pagePath": "pages/about/index"
      }
    ]
  }
}
